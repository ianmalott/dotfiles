#= AUTHOR: ianmalott (https://github.com/ianmalott)
#= DESCRIPTION: development environment for Convox Rack

PROJECTS_PATH=~/projects

new_session -c $PROJECTS_PATH/gobyexample

tmux new-window -c $PROJECTS_PATH/gobyexample -t 1 -n 'editor'
tmux new-window -c $PROJECTS_PATH/gobyexample -t 2 -n 'extra'
tmux new-window -c $PROJECTS_PATH/convox/rack -t 9 -n 'notes'

send_line 'notes' 'txt gobyexample'

select_window 'editor'

# -*- mode: sh -*-
# vi: set ft=sh :
