[alias]
  a = add
  amend = commit --amend
  b = branch
  ba = branch -a
  back = checkout -
  cm = commit
  co = checkout
  com = checkout master
  d = diff
  dc = diff --check
  ds = diff --staged
  dstat = diff --stat
  dno = diff --name-only
  f = fetch
  l = log
  l1 = log -1
  lb = log --oneline master..
  lg = log --graph --pretty=format:'%C(yellow)%h%Creset %s %C(cyan)%d %C(black) %an, %cr%Creset'
  ls = log --oneline -n20
  p = pull
  s = status
  sh = show
  shno = show --pretty='format:' --name-only
  shstat = show --stat
  touch = commit --amend --date `isodate`
  unstage = reset HEAD
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore
[push]
  default = tracking
[user]
  name = Ian Malott
  email = ian.malott@gmail.com
