[alias]
  a = add
  amend = commit --amend
  b = branch
  ba = branch -a
  br = branch -r
  back = checkout -
  cm = commit
  co = checkout
  com = checkout master
  d = diff
  dc = diff --check
  dno = diff --name-only
  ds = diff --staged
  dstat = diff --stat
  f = fetch
  files = diff --name-only master...head
  filescp = !git diff --name-only master...head | pbcopy
  head = log -1
  l = log
  l1 = log -1
  lb = log --oneline master..
  lg = log --graph --pretty=format:'%C(yellow)%h%Creset %s %C(cyan)%d %C(black) %an, %cr%Creset'
  lo = log --oneline
  ls = log --oneline -n20
  p = pull
  s = status
  sh = show
  shno = show --pretty='format:' --name-only
  shstat = show --stat
  touch = !git commit --amend --date $(isodate)
  unstage = reset HEAD
  upstream = !git for-each-ref --format='%(upstream:short)' $(git symbolic-ref -q HEAD)
[color]
  ui = auto
[color "diff"]
  meta = blue
  frag = cyan
[core]
  excludesfile = ~/.gitignore
[diff]
  tool = vimdiff
[push]
  default = upstream
[user]
  name = Ian Malott
  email = ian.malott@gmail.com
[github]
  user = ianmalott
