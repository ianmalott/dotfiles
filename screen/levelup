source $HOME/.dotfiles/screen/before

screen -t log 2 tail -f log/development.log
screen -t database 3 script/dbconsole -p
screen -t console 4 script/console
screen -t server 5 bash
  exec thin start
screen -t TODO 9 vim -p $HOME/projects/NOTES $HOME/projects/TODO $HOME/projects/DONE

source $HOME/.dotfiles/screen/after
