PROJECT_PATH=~/projects/go/src/github.com/ianmalott/monkey

new_session -c $PROJECT_PATH

tmux new-window -c $PROJECT_PATH -t 1 -n 'editor'
tmux new-window -c $PROJECT_PATH -t 2 -n 'extra'
tmux new-window -c $PROJECT_PATH -t 9 -n 'notes'

send_line 'notes' 'txt monkey'

select_window 'editor'

# -*- mode: sh -*-
# vi: set ft=sh :
